@model AppAfpaBrive.Web.Models.FileUploadModels.FileModel
@{

    @using (Html.BeginForm("Index", "FileUpload", FormMethod.Post, new { enctype = "multipart/form-data" }))


}

<form method="post" enctype="multipart/form-data" asp-controller="FileUpload" asp-action="Index">

    <div class="form-group">
        <div class="col-md-10">
            <p>Sélectionnez un ficher à importer</p>
            <input asp-for="Uploaded" type="file" name="postedFile" />
            <div asp-validation-summary="All" class="text-danger"></div>

        </div>
    </div>

    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>

</form>
